<html>
<head>
  <title>Settings</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
</head>
<body>
  <h1>Settings</h1>
  <table>
    <thead>
      <tr>
        <th>Key</th>
        <th>Value</th>
        <th>Created At</th>
        <th>Updated At</th>
      </tr>
    </thead>
    <tbody>
      <% @settings.each do |setting| %>
        <tr>
          <td><%= setting.key %></td>
          <td><%= setting.value %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</body>
</html>
